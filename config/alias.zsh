alias ..="cd ..;ls"
alias ...="cd ../..;ls"
alias ....="cd ../../..;ls"
alias .....="cd ../../../..;ls"
alias ......="cd ../../../../..;ls"

# Unix
alias ls="ls --color"
alias grep='grep --color=auto'
alias ips="ip -brief address"

